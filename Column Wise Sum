#include<iostream>
using namespace std;

int main(){

    int arr[1000][1000], m,n,i,j,sum;
    cin>>m>>n;
    for(i=0;i<m;i++)//for rows
    {
        for(j=0; j<n; j++)//for coloumns
            cin>>arr[i][j];
    }
    for(j=0; j<n; j++)
    {
    	 sum=0;
        for(i=0; i<m; i++)
            sum=sum+arr[i][j];
        cout<<sum<<' ';
    }
    
}


